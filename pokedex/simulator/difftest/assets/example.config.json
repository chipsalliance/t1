{
  "elf_path_glob": "/path/to/bin/*",
  "spike_args": [
    "--isa=rv64i",
    "--priv=m",
    "--log-commits",
    "-m0x80000000:0x20000000,0x40000000:0x20000000"
  ],
  "pokedex_args": [
    "--memory-size",
    "0xa0000000",
    "--max-same-instruction",
    "20"
  ],
  "end_pattern": {
    "action": "write",
    "memory_address": "0x40000000",
    "data": "0x1"
  }
}
